<%- include("header") -%>
    <div id="heading" class="box">
        <h1><%=listTitle%></h1>
    </div>
    <div class="box">


        <% for (var i=0;i<newlistitems.length; i++){ %>
        <div class="item">
            <input type="checkbox">
            <p><%=newlistitems[i]%></p>
        </div>
        <%} %>

        <form  class="item" action="/" method="POST">
            <input  type="text" name="newitem" placeholder="Item nou" autocomplete="off">
            <button type="submit" name="list" value=<%=listTitle%>>+</button>
        </form>
    
    </div>
<%- include("footer") -%>
   